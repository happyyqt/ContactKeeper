{
  "name": "contactkeeper",
  "version": "1.0.0",
  "description": "Contact manager app",
  "main": "server.js",
  "engines": {
    "node": "14.18.1",
    "npm": "6.14.15"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
